<h1><%= @title %></h1>

<%= form_for(@competence) do |f| %>

  <%= render 'shared/error_messages', :object => f.object %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="actions">
    <%= f.submit "Update"%>
  </div>
<% end %>

<%= link_to 'Back', competences_path %>