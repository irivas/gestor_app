<h1><%= @title %></h1>

<%= form_for(@user_competence) do |f| %>

  <%= render 'shared/error_messages', :object => f.object %>

  <div class="field">
    <%= f.label @user_competence.competence.name %><br />
    
  </div>
  <div class="field">
	<%= f.label "Level" %>
	<%= f.text_field :level %>
  </div>

  <div class="actions">
    <%= f.submit "Update"%>
  </div>
<% end %>

<%= link_to 'Back', competences_path %>